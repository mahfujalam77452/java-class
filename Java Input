public class ArrayTest {
    public static void main(String[] args) throws java.io.IOException{
      char c;
      c=(char)System.in.read();
      System.out.println(c);
      //Show if we take only one input then what will have after clicking inter.
      System.in.skip(100);
      //to skip new line for avoid taking new line as input.
      c=(char)System.in.read();
      System.out.println(c);

     
    }
}
